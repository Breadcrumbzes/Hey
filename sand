			char.Torso["RBone2"].C1 = char.Torso["RBone2"].C1:Lerp(CFrame.new(0, 0.574999988, -0.449000001, 1, 0, 0, 0, 0.905098259, 0.425202429, 0, -0.425202429, 0.905098259),i/10)
