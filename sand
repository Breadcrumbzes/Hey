				char.Torso["Left Leg"].C1 = char.Torso["Left Leg"].C1:Lerp(CFrame.new(0, 0, 0, 0.981969237, 0, -0.189040929, 0, 1, 0, 0.189040929, 0, 0.981969237),i/10)
