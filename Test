local player = game.Players.LocalPlayer
local character = player.Character

if workspace:FindFirstChild("josh") then
    workspace:FindFirstChild("josh"):Destroy()
end

local id = "rbx"
local choice = math.random(1, 2)

if choice == 1 then
    id = "rbxassetid://11637368180"
elseif choice == 2 then
    id = "rbxassetid://58550645"
end

local map = game:GetObjects(id)[1]
map.Parent = workspace
map.Name = "josh"
map:PivotTo(CFrame.new(0, 10000, 500))
character.HumanoidRootPart.CFrame = CFrame.new(0, 10100, 500)
